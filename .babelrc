{
    "presets":[
        "@babel/preset-react",
        "@babel/preset-env"
    ],
    "env": {
        "development": {
            "plugins": [
                "transform-class-properties",
                "react-hot-loader/babel",
                "babel-plugin-transform-object-assign"
            ]
        }
    },
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties",
        "@loadable/babel-plugin"
    ]/*,
    "ignore": [
        "public",
        "node_modules", 
        ".circleci",
        "src/lib/functions/build/node_modules",
        "src/__mocks__",
        "__test__",
        ".firebaserc",
        "Dockerfile",
        ".dockerignore",
        ".gitattributes",
        ".gitignore",
        "/.git",
        "firebase.json",
        "LICENSE",
        "loadable-stats.json",
        "package.json",
        "package-lock.json",
        "README.md"
    ]*/
}